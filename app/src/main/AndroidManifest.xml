<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zhntd.vault">

    <application
        android:allowBackup="true"
        android:name=".application.VaultApp"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/VaultAppTheme">
        <activity
            android:name=".activity.VaultEntryActivity"
            android:theme="@style/SecureActivityTheme"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.EmptyActivity"
            android:theme="@style/SecureActivityTheme"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>
